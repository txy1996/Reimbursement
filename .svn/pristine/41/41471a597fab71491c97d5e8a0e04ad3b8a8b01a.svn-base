public class ApplyreimVo  {
	/**number_id单号 
	 * emp_name查询报销人
	 * department_name查询部门 
	 * reim_type查询报销类型
	 * Finance_money查询报销总金额
	 */
	 
	 private int app_abb;	
	 
	 private String emp_name;
	 
	 private String department_name;
	 
	 private String reim_type;
	 
	 private int Finance_money 	;

	public ApplyreimVo(int number_id, String emp_name, String department_name, String reim_type, int finance_money) {
		super();
		this.app_abb = number_id;
		this.emp_name = emp_name;
		this.department_name = department_name;
		this.reim_type = reim_type;
		Finance_money = finance_money;
	}

	public ApplyreimVo() {
		super();
		// TODO Auto-generated constructor stub
	}

	public int getApp_add() {
		return app_abb;
	}

	public void setApp_add(int app_add) {
		this.app_abb = app_add;
	}

	public String getEmp_name() {
		return emp_name;
	}

	public void setEmp_name(String emp_name) {
		this.emp_name = emp_name;
	}

	public String getDepartment_name() {
		return department_name;
	}

	public void setDepartment_name(String department_name) {
		this.department_name = department_name;
	}

	public String getReim_type() {
		return reim_type;
	}

	public void setReim_type(String reim_type) {
		this.reim_type = reim_type;
	}

	public int getFinance_money() {
		return Finance_money;
	}

	public void setFinance_money(int finance_money) {
		Finance_money = finance_money;
	}

	@Override
	public String toString() {
		return "ApplyreimVo [app_add=" + app_abb + ", emp_name=" + emp_name + ", department_name=" + department_name
				+ ", reim_type=" + reim_type + ", Finance_money=" + Finance_money + "]";
	}
	
	 
	 
	
}
