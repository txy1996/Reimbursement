package com.zlt.system.dao;


import java.sql.SQLException;
import java.util.List;

import com.zlt.system.entity.Other;
import com.zlt.system.vo.History;

/**
 * 调用其他报销类型表信息的接口
 * @author 米邓勇
 *
 */
public interface OtherDao  extends CurrencyDao<Other>{
	/**
	 * 实现历史记录查询首页显示全部的历史记录
	 * 
	 * @param void
	 * @throws SQLException 
	 * @return历史记录集合:List<T>
	 */
	public List<History> findAllHistory() throws SQLException;

	/**
	 * 根据用户输入的模糊字段进行模糊查询
	 * 
	 * @param string
	 *            模糊字段
	 * @throws SQLException 
	 * @return相应的历史记录结果集:List<T>
	 */
	public List<History> findLike(String string) throws SQLException;

	/**
	 * 界面下拉选项选择日期范围内查询 根据下拉选传入的Sting参数只可能是当月,三个月,半年,当年,更久
	 * 
	 * @param string
	 *            日期字符串
	 * @throws SQLException 
	 * 
	 * @return相应的历史记录结果集 :List<T>
	 */
	public List<History> findByDate(String string) throws SQLException;
}
